        <attr name="main_txt" format="string" />
        <attr name="k1_txt" format="string" />
        <attr name="tools_txt" format="string" />
        <attr name="num_txt" format="string" />
        <attr name="Place_txt" format="string" />
        <attr name="people_txt" format="string" />
        <attr name="yicanjia" format="string" />
        <!--字体大小-->
        <attr name="lin1_size" format="dimension" />
        <attr name="lin2_size" format="dimension" />
        <attr name="lin3_size" format="dimension" />
        <attr name="lin4_size" format="dimension" />
        <!--字体颜色-->
        <attr name="main_TextColor" format="color" />
        <!--背景-->
       <!-- <attr name="main_Background" format="string" />-->
    </declare-styleable>
    <declare-styleable name="PowerImageView">
        <attr name="auto_play" format="boolean"></attr>
    </declare-styleable>
</resources>